{
  "name": "@acalcutt/maplibre-contour-pmtiles",
  "version": "0.1.1",
  "description": "Render contour lines from raster elevation tiles in maplibre-gl-js from pmtiles terrain sources",
  "unpkg": "dist/maplibre-contour-pmtiles.min.js",
  "main": "dist/maplibre-contour-pmtiles.cjs",
  "module": "dist/maplibre-contour-pmtiles.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "module": "./dist/maplibre-contour-pmtiles.mjs",
    "require": "./dist/maplibre-contour-pmtiles.cjs",
    "types": "./dist/index.d.ts",
    "browser": "./dist/maplibre-contour-pmtiles.min.js"
  },
  "scripts": {
    "bench": "npm run build && node benchmark.js",
    "build": "rm -rf dist && cross-env NODE_ENV=web rollup -c && npm run generate-types && tsc --project tsconfig.dist.json",
    "test-build-v3": "cd test-project-maplibre-3 && rm -rf node_modules && rm -rf package-lock.json && npm i && npm test",
    "test-build": "cd test-project && rm -rf node_modules && rm -rf package-lock.json && npm i && npm test",
    "generate-types": "tsc --emitDeclarationOnly --declaration --outDir dist && rm -rf dist/*{.t,-j}est.d.ts",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "test": "run-p typecheck lint jest checkformat",
    "lint": "eslint src",
    "format": "prettier src --write",
    "checkformat": "prettier src --check",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onthegomap/maplibre-contour.git"
  },
  "type": "module",
  "author": "Michael Barry",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/onthegomap/maplibre-contour",
  "files": [
    "build/",
    "dist/*",
    "src/"
  ],
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.17.0",
    "@mapbox/tilebelt": "^2.0.2",
    "@mapbox/vector-tile": "2.0.3",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "28.0.2",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.2",
    "@types/benchmark": "2.1.5",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.13",
    "@types/mapbox__vector-tile": "1.3.4",
    "@types/node": "22.10.2",
    "@types/pbf": "3.0.5",
    "@types/pngjs": "6.0.5",
    "benchmark": "2.1.4",
    "canvas": "^2.11.2",
    "commander": "^13.0.0",
    "cross-env": "^7.0.3",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lodash": "4.17.21",
    "npm-run-all": "4.1.5",
    "pbf": "4.0.1",
    "pmtiles": "^4.1.0",
    "pngjs": "7.0.0",
    "prettier": "3.4.2",
    "rollup": "4.28.1",
    "sharp": "^0.33.5",
    "ts-jest": "29.2.5",
    "tslib": "^2.8.1",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.2",
    "whatwg-fetch": "3.6.20"
  }
}
